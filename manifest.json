{
	"manifest_version": 3,
	"version": "1.1.0",
	"name": "GH Real Names",
	"author": "Tom",
	"description": "Replace IDs with real names on Github",
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content.js"
			],
			"run_at": "document_idle",
			"all_frames": false
		}
	],
	"icons": {
		"48": "assets/icon48.png",
		"96": "assets/icon96.png"
	}
}
